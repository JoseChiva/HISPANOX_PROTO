#--/
#  recherche du traitement de localisation des arrondis
Read [TCY]TCY0 = [F:CPY]CRY
If fstat : Raz [F:TCY] : End : Endif
If [F:TCY]CTLPRG<>""
  Local Integer OK
  Call EXISTE_ADX("","",[F:TCY]CTLPRG,OK) From ORDSYS
  If OK
    GVARCTLPRG=[F:TCY]CTLPRG
  Endif
Endif

End
