If YN = 2 Then
  For I = 0 To [M:POH2]NBLIG-1
    If [M:POH2]LINCLEFLG(I) <> 2
      [M:POH2]DEMRCPDAT(I) = VALEUR
      [M:POH2]UPDFLG(I) = 2
      Affzo [M:POH2]DEMRCPDAT(I)
    Endif
  Next
Endif

End

