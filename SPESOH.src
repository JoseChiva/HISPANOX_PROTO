  [M:ZSOH1]ZCOSTOFEREK([L]I_NUM_REG)= [XSQD]ZCOSTOFEREK
  [M:ZSOH1]ZLINOCU([L]I_NUM_REG)    = [XSQD]ZLINOCU
  If fstat = 0 Then
    [M:ZSOH1]VLYDAT([L]I_NUM_REG)   = [XSQH]VLYDAT
  Endif
  [M:ZSOH1]NBLIG2 = I_NUM_REG + 1

