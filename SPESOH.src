    Endif
    Affzo [M:SOH4]NBLIG(nolign-1)
    GCTRLSTA = 0
  Endif
# JC.08072021.vtas029.END

#If GUSER="ADEV" Then : Infbox "salgo am_zcosofer"-num$([M:SOH4]GROPRI(nolign-1)) : Endif
