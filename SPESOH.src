    If [M:SOH1]HLDSTA = 2 Then
      If [M:SOH1]HLDCOD <> "PORTES" Then
        [M:SOH1]HLDSTA = 2
        [M:SOH1]HLDCOD = "PORTESYOTROS"
        Affzo [M:SOH1]HLDSTA
        Affzo [M:SOH1]HLDCOD
      Endif
    Else
      [M:SOH1]HLDSTA = 2
      [M:SOH1]HLDCOD = "PORTES"
      Affzo [M:SOH1]HLDSTA
      Affzo [M:SOH1]HLDCOD
    Endif
  # desbloqueo
  Else
    If [M:SOH1]HLDSTA = 2 Then
      If [M:SOH1]HLDCOD = "PORTES" Then
        [M:SOH1]HLDSTA = 1
        [M:SOH1]HLDCOD = ""
        Affzo [M:SOH1]HLDSTA
        Affzo [M:SOH1]HLDCOD
      Elsif [M:SOH1]HLDCOD = "PORTESYOTROS" Then
        [M:SOH1]HLDSTA = 2
        [M:SOH1]HLDCOD = "OTROS"
        Affzo [M:SOH1]HLDSTA
