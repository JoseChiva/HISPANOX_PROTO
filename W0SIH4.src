  indice = nolign-1
  [M]NBLIGV = nolign
  status = 75
  Call SAI_NBLIGV(IMPFIC,ACTION,IMPTRT)
  If mkstat : Break : Endif
 Next nolign
 If mkstat : End : Endif
 indice = 0
Endif

mkstat = 0

End

