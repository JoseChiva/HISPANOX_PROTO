GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M:SIH0]CUR,GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]INVATI : Endif

####################################
## Bloc tableau 4
####################################
IMPTBL = 1

indice = 0
status = 1
zoncou = "NBLIGV"
If !mkstat
 NBLIG = [M]NBLIGV
