Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
SAVACT = GACTION : GACTION = "DIVLINCONT"
Call INSERE("SOPLIN") From TABLEAUX
GACTION = SAVACT
If mkstat : GERR = 1 : GZONE = "NBLIG" : GZNUM = 0 : Endif
Gosub AFF From GSAISIE
If mkstat : End : Endif
If !mkstat
 SAVACT = GACTION : GACTION = "STD"
 Call C_NBLIG From SUBSOH
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "NBLIG" : GZNUM = 0 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "SPE"
  Call C_NBLIG From SPESOH
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "NBLIG" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  SAVACT = GACTION : GACTION = "STD"
  Call AVANT_NBLIG From SUBSOH
  GACTION = SAVACT
  If !mkstat
   SAVACT = GACTION : GACTION = "INIECCSTA"
   Call INIECCSTA([M:SOH4]ITMREF(nolign-1),"SOH4",nolign-1) From STKECR
   GACTION = SAVACT
   If !mkstat
    SAVACT = GACTION : GACTION = "SPE"
    Call AVANT_NBLIG From SPESOH
    GACTION = SAVACT
   Endif
  Endif
 Endif
Endif

