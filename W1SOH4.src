Subprog APRES_NBLIG
Local Decimal BASPAG
## Action DIVLINNUM
Call LINNUM("SOPLIN") From TABLEAUX
If mkstat : End : Endif
## Action SOHP1NBLIG
Local Char    PARMSK(250)(1..20) , SAVACT(20)
SAVACT = GACTION : GACTION = "SOHP1NBLIG"
Call S_AFF_NUM(BASPAG,PARMSK,"P1NBLIG","SUBSOH","") From GACTION
GACTION = SAVACT
If mkstat : End : Endif
If !GWEBSERV
 ## Action TRTCRENOM
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "TRTCRENOM"
 PARMSK(1) = [M:SOH4]ITMREF(nolign-1)
 PARMSK(2) = num$(nolign-1)
 PARMSK(3) = [M:SOH0]ORDDAT
 PARMSK(4) = "2"
 PARMSK(5) = num$(GBIDI1)
 PARMSK(6) = num$([M:SOH4]SOPLIN(nolign-1))
 PARMSK(7) = GBIDC1
 PARMSK(8) = num$(GBIDI1)
 PARMSK(9) = num$(GBIDI1)
 Call TRAITE_NUM(BASPAG,PARMSK,"","TRTVENBOM","ISELBOD",4,"XYTRTVENBOM") From GTRAITE
 GBIDC1 = PARMSK(7)
 GBIDI1 = val(PARMSK(8))
 GBIDI1 = val(PARMSK(9))
 GACTION = SAVACT
 If mkstat : End : Endif
Endif
If  GWEBSERV
 ## Action IMPCRENOM
 Local Integer OKTRT
 Call EXISTE_ADX("","","XYTRTVENBOM",OKTRT) From ORDSYS
 If OKTRT>0
  Call IMPCRENOM([M:SOH4]ITMREF(nolign-1),nolign-1,[M:SOH0]ORDDAT,2,GBIDI1,[M:SOH4]SOPLIN(nolign-1),GBIDC1,GBIDI1
&  ,GBIDI1) From XYTRTVENBOM
 Else
  Call IMPCRENOM([M:SOH4]ITMREF(nolign-1),nolign-1,[M:SOH0]ORDDAT,2,GBIDI1,[M:SOH4]SOPLIN(nolign-1),GBIDC1,GBIDI1
&  ,GBIDI1) From TRTVENBOM
 Endif
 If mkstat : End : Endif
Endif
## Action SOHP2NBLIG
Local Char    PARMSK(250)(1..20) , SAVACT(20)
SAVACT = GACTION : GACTION = "SOHP2NBLIG"
Call S_AFF_NUM(BASPAG,PARMSK,"P2NBLIG","SUBSOH","") From GACTION
GACTION = SAVACT
If mkstat : End : Endif
Gosub _AFF
End

