 ## Action STD
 Call AS_DISCRGVAL1(VALEUR) From SUBSOH
 If mkstat=2
  [M]DISCRGVAL1(INDIC) = VALEUR
  Affzo DISCRGVAL1(INDIC) : mkstat = 2
 Endif
 If !mkstat
  ## Action DIVREN
  Local Char    PARMSK(250)(1..20) , SAVACT(20)
  SAVACT = GACTION : GACTION = "DIVREN"
  PARMSK(1) = "SOH4"
  PARMSK(2) = "5"
