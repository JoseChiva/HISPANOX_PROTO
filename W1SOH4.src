  PARMSK(4) = "DISCRGVAL1"
  Call S_AFF_NUM(VALEUR,PARMSK,"DIVREN","TRTPRICE","") From GACTION
  GACTION = SAVACT
  If mkstat=2
   [M]DISCRGVAL1(INDIC) = VALEUR
   Affzo DISCRGVAL1(INDIC) : mkstat = 2
  Endif
  If !mkstat
   ## Action SPE
   Call AS_DISCRGVAL1(VALEUR) From SPESOH
   If mkstat=2
    [M]DISCRGVAL1(INDIC) = VALEUR
    Affzo DISCRGVAL1(INDIC) : mkstat = 2
   Endif
   Onkey 130 Titled GBOUT2 Call B2_DISCRGVAL1(zc) From W1SOH4
