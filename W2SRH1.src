 PARMSK(2) = "12"
 PARMSK(3) = [M:SRH0]TRSCOD
 PARMSK(4) = [M]STOFCY
 PARMSK(5) = [M:SRH1]ITMREF(nolign-1)
 PARMSK(6) = [M:SRH1]SAU(nolign-1)
 PARMSK(7) = num$([M:SRH1]QTY(nolign-1))
 PARMSK(8) = [M:SRH1]STA(nolign-1)
 PARMSK(9) = [M:SRH1]LOCTYP(nolign-1)
 PARMSK(10) = VALEUR
 PARMSK(11) = [M:SRH0]BPCORD
 PARMSK(12) = string$([M:SRH1]STA(nolign-1)="","S")
 PARMSK(13) = ""
 PARMSK(14) = string$(GWRHACT-1,num$(sigma(1,min(1,dim([M:SRH1]WRH)),[M:SRH1]WRH(nolign-1))))
 Call SAISIE_CHAR(VALEUR,PARMSK,"ISELEMP","STKLOC","XWISELEMP") From GSAISIE
 [M]WLOCSEQ(nolign-1) = val(PARMSK(1))
