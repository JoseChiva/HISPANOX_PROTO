Onevent BEFORE_INPUT Call AS_ORDINVATI(zc) From W4SOH4
Return

Subprog IB_ORDINVATI(VALEUR)
Variable Decimal VALEUR
## Action STD
Call IB_ORDINVATI From SUBSOH
Gosub _AFF
End

$AV_ORDINVATI
## Action INIDEV
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M:SOH0]CUR,GDEVISE(1..GNBDEV)),1))
Chgfmt [M]ORDINVATI(indice) With "Nz3:"+GDEVFMT
Gosub _AFF
Return

Subprog AS_ORDINVATI(VALEUR)
Variable Decimal VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(25407,GBOUT1) From OBJDIV
Call IB_ORDINVATI(VALEUR)
Onkey 129 Titled GBOUT1 Call B1_ORDINVATI(zc) From W4SOH4
Gosub _AFF
End

Subprog B1_ORDINVATI(VALEUR)
Variable Decimal VALEUR
If !GWEBSERV
 ## Action VISUVALTTC
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "VISUVALTTC"
 PARMSK(1) = "GESSOH"
 Call SAISIE_NUM(VALEUR,PARMSK,"IVTTC","TRTVENFACR","XWIVTTC") From GSAISIE
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

$ON_WOBJ1
Return

$ON_WQTY
Onevent BEFORE_AREA Gosub AV_WQTY From W4SOH4
Return

$AV_WQTY
If !GWEBSERV
 ## Action FMTQTY
 GUNTFMT="15F" : GUNTFMT = GUNTFMT1(max(find("",GUNITE(1..GNBUNT)),1))
Endif
Chgfmt [M]WQTY(indice) With "NPz:"+GUNTFMT
Gosub _AFF
Return

$TIT_COL
Local Char TITCL(200)(1..23)
Raz TITCL
Case BASPAG
 When "NBLIG"
  TITCL(1) = GCOLREM(0)
