  TITCL(10) = GCOLDIE(7)
  TITCL(11) = GCOLDIE(8)
  TITCL(12) = GCOLDIE(9)
  TITCL(13) = GCOLDIE(10)
  TITCL(14) = GCOLDIE(11)
  TITCL(15) = GCOLDIE(12)
  TITCL(16) = GCOLDIE(13)
  TITCL(17) = GCOLDIE(14)
  TITCL(18) = GCOLDIE(15)
  TITCL(19) = GCOLDIE(16)
