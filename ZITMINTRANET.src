  If !fstat
    TEXTO=[F:AXX]TEXTE
  Endif
Filter [F:AXX]

Filter [F:AXX1] Where CODFIC = 'ATABDIV' and ZONE = 'LNGDES' and LANGUE = 'SPA' and IDENT1 = '21' and IDENT2 = [F:ITM]TSICOD(1)
