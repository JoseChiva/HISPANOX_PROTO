Filter [F:AXX2]

Filter [F:AXX3] Where CODFIC = 'ATABDIV' and ZONE = 'LNGDES' and LANGUE = 'SPA' and IDENT1 = '23' and IDENT2 = [F:ITM]TSICOD(3)
Read  [F:AXX3] First
  If !fstat
    TEXTO3=[F:AXX3]TEXTE
  Endif
Filter [F:AXX3]

#COSTES
Filter [F:ITF] Where ITMREF = [F:ITM]ITMREF and STOFCY = 'PHISP'
Read  [F:ITF] First
If !fstat
    ZCMO=[F:ITF]ZCMO*100
    ZCMREP=[F:ITF]ZCMREP*100
    ZSAFSTO=[F:ITF]SAFSTO
Endif
Filter [F:ITF]

#CALCULOS
Filter [F:ITV] Where ITMREF = [F:ITM]ITMREF and STOFCY = 'PHISP'
Read  [F:ITV] First
If !fstat
    STOCK_REAL=[F:ITV]PHYSTO+[F:ITV]CTLSTO+[F:ITV]REJSTO
    STOCK_DISP=[F:ITV]PHYSTO-[F:ITV]GLOALL-[F:ITV]PHYALL-[F:ITV]GLOSHT
    STOCK_MIN=[F:ITV]SALSTO
    COSTE_MEDIO=[F:ITV]AVC*100
    #ZCOSTE=[F:ITV]LASRCPPRI*100
    #06.304.004 - coste última recepción
    # 06.319.601.AUR.STR siempre mostrar precio última-compra
    #str.auren 03/02/2022
#    If [F:ITV]LASPURPRI=0 Then
#      ZCOSTE=[F:ITV]LASRCPPRI*100
#    Else
#      ZCOSTE=[F:ITV]LASPURPRI*100
#    Endif
    ZCOSTE=[F:ITV]LASPURPRI*100
    #fin.auren 03/02/2022
    If [F:ITM]ITMWEI>0
      COSTE_M_KG=([F:ITV]AVC/[F:ITM]ITMWEI)*1000
    Else
      COSTE_M_KG=0
    Endif
Endif
Filter [F:ITV]


#COEFICIENTE
If [F:ITM]PCU(0)="CAJ"
    CAJA=[F:ITM]PCUSTUCOE(0)
Elsif [F:ITM]PCU(1)="CAJ"
    CAJA=[F:ITM]PCUSTUCOE(1)
Endif

If [F:ITM]PCU(0)="PAC"
    PACK=[F:ITM]PCUSTUCOE(0)
Elsif  [F:ITM]PCU(1)="PAC"
    PACK=[F:ITM]PCUSTUCOE(1)
Endif


Case [F:ITM]WEU
  When "G"  : ITMWEI=([F:ITM]ITMWEI/1000)*1000
  When "HG" : ITMWEI=([F:ITM]ITMWEI/10)*1000
  When "KG" : ITMWEI=([F:ITM]ITMWEI)*1000
  When "LB" : ITMWEI=([F:ITM]ITMWEI/2.205)*1000
  When "MG" : ITMWEI=([F:ITM]ITMWEI/1000000)*1000
  When "OZ" : ITMWEI=([F:ITM]ITMWEI/35.274)*1000
  When "T"  : ITMWEI=([F:ITM]ITMWEI/0.001)*1000
  When "QT" : ITMWEI=([F:ITM]ITMWEI/0.01)*1000
Endcase


WDAT=gdat$(1,month(date$),year(date$))


Filter [F:ITH] Where ITMREF = [F:ITM]ITMREF and STOFCY = 'PHISP' and PERSTR=WDAT
  For [F:ITH]
    If !fstat
        SALIDA_IMP=SALIDA_IMP+[F:ITH]MONISSQTY
    Endif
  Next
Filter[F:ITH]

Filter [F:ITH] Where ITMREF = [F:ITM]ITMREF and STOFCY = 'PHISP' and PERSTR>=WDAT
  For [F:ITH]
    If !fstat
        ENTRADA_A=ENTRADA_A+[F:ITH]YEARCPQTY
    Endif
  Next
Filter[F:ITH]

#ULTIMA COMPRA - CODIGO PROVEEDOR
Filter [F:PTD] Where ITMREF=[F:ITM]ITMREF Order By RCPDAT Desc
Read[F:PTD]First
  If !fstat
      ZBPSNUM=[F:PTD]BPSNUM
      ZRCPDAT=[F:PTD]RCPDAT
      #ZCOSTE=([F:PTD]CPRCOE*[F:PTD]NETPRI)*100
  Endif

ZBPSSHO=""
If ZBPSNUM<>"" Then
#MOTE PROVEEDOR
Filter [F:BPS] Where BPSNUM=[F:PTD]BPSNUM
Read[F:BPS]First
    If !fstat
        ZBPSSHO=[F:BPS]BPSSHO
    Endif
Endif
Filter [F:BPS]
Filter [F:PTD]


#####NO METEMOS ESTO EN LA VISTA########################################################################

#TABLA ZPRESPD DETALLE ENTRADA RESPUESTAS ZPPD
#BUSCAMOS FECHA ULTIMA OFERTA AL PROVEEDOR Y LUEGO BUSCAMOS LA FECHA DE 30 DÍAS MENOS Y VEMOS LA OFERTA
Filter [F:ZPPD] Where ITMREF=[F:ITM]ITMREF and LINRSPDAT<>[00/00/0000] and PRI>0 Order By LINRSPDAT Desc
Read[F:ZPPD] First
    If !fstat
        WDAT1=[F:ZPPD]LINRSPDAT
        WDAT2=WDAT1-30
    Endif
Filter [F:ZPPD]

#ENTRE ZWDAT Y LINRSPDAT VER LA MEJOR OFERTA
# 06.319.601.ini
#Filter [F:ZPPD] Where ITMREF=[F:ITM]ITMREF and (LINRSPDAT<=WDAT1 or LINRSPDAT>=WDAT2) Order By PRI Asc
#Filter [F:ZPPD] Where ITMREF=[F:ITM]ITMREF and (LINRSPDAT<=WDAT1 or LINRSPDAT>=WDAT2) and PRI>0 Order By PRI Asc
# 06.319.601.fin
# 06.323.891.INI CAMBIAMOS ORDER A HXCOST
Filter [F:ZPPD] Where ITMREF=[F:ITM]ITMREF and (LINRSPDAT<=WDAT1 or LINRSPDAT>=WDAT2) and PRI>0 Order By HXCOST Asc
# 06.323.891.FIN
Read[F:ZPPD] First
    If !fstat
        ZBPSNUMOF=[F:ZPPD]BPSNUM
        ZLINRSPDAT=[F:ZPPD]LINRSPDAT
        ZHXCOST=[F:ZPPD]HXCOST
    Endif
Filter [F:ZPPD]


ZBPSSHO_OF=""
#MOTE PROVEEDOR MEJOR OFERTA
Filter [F:BPS] Where BPSNUM=ZBPSNUMOF
Read[F:BPS]First
    If !fstat
        ZBPSSHO_OF=[F:BPS]BPSSHO
    Endif
Filter [F:BPS]


#ESTADÍSTICAS
XWDAT1=(date$-365)

Filter [F:SAT] Where COD="ZS12M" and CRI1=[F:ITM]ITMREF and DAT>=XWDAT1
     For [F:SAT]
         If !fstat
            SALIDA_12=SALIDA_12+[F:SAT]AMT(0)
            SALIDA_LIN=SALIDA_LIN+[F:SAT]AMT(1)
         Endif
     Next
Filter [F:SAT]

Filter [F:SAT] Where COD="ZS12D" and CRI1=[F:ITM]ITMREF and DAT>=XWDAT1 Order By AMT Desc
     Read [F:SAT]First
         If !fstat
            SALIDA_MAX=[F:SAT]AMT(0)
         Endif
Filter [F:SAT]


Filter [F:SAT] Where COD="ZOFEX" and CRI1=[F:ITM]ITMREF and DAT>=XWDAT1
     For [F:SAT]
         If !fstat
             OF_NO_VEN=OF_NO_VEN+[F:SAT]AMT(0)
         Endif
     Next
Filter [F:SAT]

