$DATOS_ARTICULO
Local Char DESCRIPCION2(250), DESCRIPCION3(250), DESCARTCLI(250), REFERENCIA(250),LBPSLOT(250),BPSNUML(250)
Local Char LOTE(250),UBIC(250),ZMASCARA(250),ALMACEN(250)

LBPSLOT = [F:SDDZ]ZLOTPROVSGA

Read [F:ITMZ]ITM0=[F:SDDZ]ITMREF
Filter [F:ITUZ] Where ITMREF = [F:SDDZ]ITMREF and BPCNUM = [F:SDDZ]BPCORD
Read [F:ITUZ] First
If !fstat Then
  DESCARTCLI = [F:ITUZ]ITMDESBPC
  REFERENCIA = [F:ITUZ]ITMREFBPC
Endif

Filter [F:ITMZ] Where ITMREF = [F:SDDZ]ITMREF
Read [F:ITMZ] First
If !fstat Then
  DESCRIPCION2 = [F:ITMZ]ITMDES2
  DESCRIPCION3 = [F:ITMZ]ITMDES3
Endif

If [F:SDHZ]CFMFLG = 2
  Filter [F:STJZ] Where VCRNUM = [F:SDDZ]SDHNUM and VCRLIN = [F:SDDZ]SDDLIN
  Read [F:STJZ] First
  If !fstat
#    LBPSLOT  = [F:STJ]BPSLOT
    If rowcount([F:STJZ]) = 1 Then
      LOTE    = [F:STJZ]LOT
    Endif
    UBIC    = [F:STJZ]LOC
#    Filter [F:STL] Where ITMREF = [F:SDD]ITMREF and LOT = [F:STJ]LOT
    If LBPSLOT <> "" Then
      Filter [F:STLZ] Where ITMREF = [F:SDDZ]ITMREF and BPSLOT = LBPSLOT
      Read [F:STLZ] First
      If !fstat
        BPSNUML = [F:STLZ]BPSNUM
      Else
        BPSNUML = ""
      Endif
    Endif
  Endif
Else
  Filter [F:STAZ] Where VCRNUM = [F:SDDZ]SDHNUM and VCRLIN = [F:SDDZ]SDDLIN
  Read [F:STAZ] First
  If !fstat
    Filter [F:STOZ] Where ITMREF = [F:STAZ]ITMREF and STOCOU = [F:STAZ]STOCOU and STOFCY = [F:STAZ]STOFCY
    Read [F:STOZ]First
    If !fstat
#      LBPSLOT   =  [F:ZMAZ]BPSLOT
      If rowcount([F:STAZ]) = 1 Then
        LOTE     =  [F:STOZ]LOT
      Endif
      UBIC     =  [F:STOZ]LOC
      ALMACEN  =  [F:STOZ]LOCTYP
#      Filter [F:STL] Where ITMREF = [F:SDD]ITMREF and LOT = [L]LOTE
      If LBPSLOT <> "" Then
        Filter [F:STLZ] Where ITMREF = [F:SDDZ]ITMREF and BPSLOT = LBPSLOT
        Read [F:STLZ] First
        If !fstat
           BPSNUML = [F:STLZ]BPSNUM
         Else
           BPSNUML = ""
        Endif
      Endif
    Endif
  Endif

Endif


Read [F:ZMAZ]ZMA0 = [F:ITMZ]ZMASK
If !fstat
  [L]ZMASCARA = [F:ZMAZ]ZDESMAS1
Endif

Return

##################################################################
$UNIDAD_PRECIO
Local Char    LUNIDADPRECIO(250)

  If !clalev([F:ZSOP]) Then : Local File SORDERP [F:ZSOP] : Endif

  If [F:SDDZ]ZUNPRE <> "" Then
    Case [F:SDDZ]ZUNPRE
      When "UN"     : LUNIDADPRECIO = "u"
      When "CEN"    : LUNIDADPRECIO = "%"
      When "MIL"    : LUNIDADPRECIO = "m"
    Endcase
  # si el campo no está en la línea de la entrega, debe coger el valor de este campo en el pedido
  Else
    Read [F:ZSOP]SOP3 = [F:SDDZ]SOHNUM;[F:SDDZ]SOPLIN
    If !fstat Then
      Case [F:ZSOP]ZUNPRE
        When "UN"     : LUNIDADPRECIO = "u"
        When "CEN"    : LUNIDADPRECIO = "%"
        When "MIL"    : LUNIDADPRECIO = "m"
        When Default  : LUNIDADPRECIO = ""
    Endcase
    Endif
  Endif

  Close Local File [ZSOP]
Return

